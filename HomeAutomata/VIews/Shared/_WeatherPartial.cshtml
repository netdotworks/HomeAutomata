@using HomeAutomata.Services.HttpServices.Weather
@inject IWeatherService WeatherService
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    var currentWeather = await WeatherService.GetCurrentWeather();
}

<div>Temp: @currentWeather.Main.Temp <sup>o</sup>C</div>
<div>Temp Real Feel: @currentWeather.Main.FeelsLike <sup>o</sup>C</div>